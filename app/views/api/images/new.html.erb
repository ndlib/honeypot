<h1>Test FileUploader</h1>

<%= form_tag(api_images_path, multipart: true, method: :post) do %>
  <p>
    <%= label_tag(:image, "Image") %>
    <%= file_field_tag(:image) %>
  </p>
  <p>
    <%= label_tag(:application_id, "Application ID") %>
    <%= text_field_tag(:application_id, "test") %>
  </p>
  <p>
    <%= label_tag(:group_id, "Group ID") %>
    <%= text_field_tag(:group_id, 1) %>
  </p>
  <p>
    <%= label_tag(:item_id, "Item ID") %>
    <%= text_field_tag(:item_id, 1) %>
  </p>
  <%= submit_tag("Upload") %>
<% end %>
